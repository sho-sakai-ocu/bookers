<h1>投稿一覧</h1>
<% @books.each do |book| %>
  <p>タイトル</p>
  <span><%= book.title %></span>


<h1>New book</h1>
<!-- form_with部分 -->
<%= form_with model: @book do |f| %>
<h1>ここはnewです</h1>
  <h4>Title</h4>
  <%= f.text_field :title %>
	
  <h4>body</h4>
  <%= f.text_area :body %>
	
  <%= f.submit 'Create Book' %>
<% end %>